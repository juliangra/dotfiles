[user]
	name = Julian
	email = juliangr@stud.ntnu.no

[init]
	defaultBranch = main

[url "git@bitbucket.org:"]
	insteadof = https://bitbucket.org/

[commit]
	gpgsign = false

[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only

[delta]
  navigate = true

[merge]
  conflictStyle = diff3

[diff]
  colorMoved = default
[gpg]
	format = ssh
[tag]
	gpgsign = false
